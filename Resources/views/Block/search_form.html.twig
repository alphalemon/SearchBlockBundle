<form action="{{ search_route }}" method="post" {{ form_enctype(search_form) }}>
    {{ form_errors(search_form) }}

    <td>{{ form_label(search_form.searchText, "Search" | trans([], 'search')) }}</td>
    <td colspan="2">{{ form_widget(search_form.searchText) }}</td>
    <td colspan="2"><button> </button></td>
    {{ form_rest(search_form) }}
</form>